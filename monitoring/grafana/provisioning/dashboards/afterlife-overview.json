{
  "dashboard": {
    "id": null,
    "title": "AfterLife API Overview",
    "tags": ["afterlife", "fastapi", "overview"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "API Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"backend\"}[5m])",
            "legendFormat": "{{method}} {{handler}}"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        }
      },
      {
        "id": 2,
        "title": "API Response Time (95th percentile)",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"backend\"}[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 3,
        "title": "Active Sessions",
        "type": "stat",
        "targets": [
          {
            "expr": "http_requests_total{job=\"backend\",handler=\"/upload\"}",
            "legendFormat": "Upload Sessions"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 4,
        "title": "HTTP Status Codes",
        "type": "piechart",
        "targets": [
          {
            "expr": "increase(http_requests_total{job=\"backend\"}[1h])",
            "legendFormat": "{{status_code}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 5,
        "title": "Request Duration by Endpoint",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(http_request_duration_seconds_bucket{job=\"backend\"}[5m])",
            "legendFormat": "{{handler}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 6,
        "title": "Memory Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"backend\"}",
            "legendFormat": "Backend Memory"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16},
        "fieldConfig": {
          "defaults": {
            "unit": "bytes"
          }
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "10s",
    "schemaVersion": 30,
    "version": 1
  }
}